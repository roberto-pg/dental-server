{
  "parser": "@typescript-eslint/parser",
  "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "prettier/prettier": "error"
      // "camelcase": "off",
      // "no-useless-constructor": "off",
      // "no-use-before-define": "off"
  }
}

